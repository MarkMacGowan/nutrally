<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".itemList">

    <ImageView
        android:id="@+id/logoImg2"
        android:layout_width="241dp"
        android:layout_height="138dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="82dp"
        android:layout_marginBottom="646dp"
        android:scaleX="2"
        android:scaleY="2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher_foreground" />

    <Button
        android:id="@+id/returnToHomeBtn"
        android:layout_width="350dp"
        android:layout_height="109dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="660dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="17dp"
        android:backgroundTint="#51C956"
        android:text="Return "
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/foodListLbl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="143dp"
        android:layout_marginTop="133dp"
        android:layout_marginEnd="143dp"
        android:layout_marginBottom="613dp"
        android:text="Food List"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.chip.Chip
        android:id="@+id/listItemBtn1"
        android:layout_width="231dp"
        android:layout_height="69dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="189dp"
        android:layout_marginEnd="149dp"
        android:layout_marginBottom="528dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>